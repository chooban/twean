{
	"manifest_version": 2,
	"name": "Twean",
	"short_name": "Twean",
	"version": "1.1.0",
	"description": "Remove auto-expanded images from your Twitter timeline.",
	"author":"Tekool - Frédéric Saunier",
	"homepage_url" :"https://github.com/tekool/twean",

	"content_scripts":
	[
		{
			"matches": ["https://twitter.com/"],
			"js": ["page.js"]
		}
	],

	"browser_action":
	{
		"default_title": "Twean"
	}
}