{
  "name": "Twean",
  "version": "1.0",
  "description": "Clean your Twitter timeline by removing inline images.",
  "content_scripts": [
    {
      "matches": ["https://twitter.com/"],
      "js": ["page.js"]
    }
  ],
  "browser_action": {
    "default_title": "Twean",
    "default_icon": "twean.png"
  },
  "manifest_version": 2
}
